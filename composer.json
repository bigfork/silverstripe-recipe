{
    "name": "bigfork/silverstripe-recipe",
    "type": "silverstripe-recipe",
    "description": "Bigforkâ€™s SilverStripe CMS recipe",
    "homepage": "https://www.bigfork.co.uk",
    "license": "BSD-3-Clause",
    "require": {
        "bigfork/silverstripe-fail-whale": "^4",
		"bigfork/silverstripe-formtacular": "^2",
		"bigfork/silverstripe-grouped-cms-menu-fork": "^5",
        "bigfork/silverstripe-image-recipe": "^4",
        "bigfork/silverstripe-opengraph-fork": "^2",
        "bigfork/silverstripe-sentry-handler": "^3",
		"bigfork/silverstripe-share-care-fork": "^1",
        "bigfork/silverstripe-simpleseo": "^5",
        "bigfork/silverstripe-textdropdownfield-fork": "^2",
        "bigfork/silverstripe-vitesse": "^2",
        "bigfork/silverstripe-vitesse-spritemap": "^0",
        "bummzack/sortablefile": "^3",
        "dnadesign/silverstripe-elemental": "^6",
        "kinglozzer/metatitle": "^4",
        "silverstripe/asset-admin": "^3",
        "silverstripe/assets": "^3",
        "silverstripe/cms": "^6",
        "silverstripe/config": "^3",
        "silverstripe/framework": "^6",
        "silverstripe/graphql": "^6",
		"silverstripe/htmleditor-tinymce": "^1",
        "silverstripe/login-forms": "^6",
        "silverstripe/mfa": "^6",
        "silverstripe/mimevalidator": "^4",
        "silverstripe/recipe-plugin": "^2",
        "silverstripe/redirectedurls": "^4",
        "silverstripe/sharedraftcontent": "^4",
        "silverstripe/siteconfig": "^6",
        "silverstripe/totp-authenticator": "^6",
        "symbiote/silverstripe-gridfieldextensions": "^5",
        "wilr/silverstripe-googlesitemaps": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^11",
        "symfony/var-dumper": "^7"
    },
    "extra": {
        "expose": [
            "app/css",
            "app/lang",
            "themes/default/dist"
        ],
        "resources-dir": "_resources"
    },
    "autoload": {
        "files": [
            "app/src/bootstrap.php"
        ],
        "psr-4": {
            "App\\": "app/src",
            "Bigfork\\": "util/Bigfork"
        }
    },
    "scripts": {
        "post-create-project-cmd": "Bigfork\\Installer\\Install::postCreateProject"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "php-http/discovery": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true
        },
        "sort-packages": true
    }
}
