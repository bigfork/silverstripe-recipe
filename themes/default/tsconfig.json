{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2020",
    "noImplicitAny": false,
    "sourceMap": false,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedSideEffectImports": true
  },
  "include": ["src/js/**/*"],
}
